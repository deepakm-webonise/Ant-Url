%h1 Listing hits

%table
  %thead
    %tr
      %th Latitude
      %th Longitude
      %th Country
      %th Ant
      %th
      %th
      %th

  %tbody
    - @hits.each do |hit|
      %tr
        %td= hit.latitude
        %td= hit.longitude
        %td= hit.country
        %td= hit.ant
        %td= link_to 'Show', hit
        %td= link_to 'Edit', edit_hit_path(hit)
        %td= link_to 'Destroy', hit, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Hit', new_hit_path
